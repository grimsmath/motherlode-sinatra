<div class="panel panel-default">
  <div class="panel-heading">
    <h3 style="margin-top: 0">Edit User Profile</h3>
    <p>From here you can make changes to the selected user</p>
  </div>
  <div class="panel-body">
    <form role="form">
      <fieldset>
        <div class="form-group">
            <label for="username">Username: </label>
            <input type="text" class="form-control" id="username" placeholder="">
        </div>

        <div class="form-group">
            <label for="Password">Password: </label>
            <input type="password" class="form-control" id="Password" placeholder="">
        </div>

        <div class="form-group">
            <label for="ConfirmPassword">Confirm Password: </label>
            <input type="password" class="form-control" id="ConfirmPassword" placeholder="">
        </div>
      </fieldset>

      <fieldset>
        <div class="form-group">
            <label for="FirstName">Firstname:</label>
            <input type="text" class="form-control" id="FirstName" placeholder="">
        </div>

        <div class="form-group">
            <label for="LastName">Lastname:</label>
            <input type="text" class="form-control" id="LastName" placeholder="">
        </div>

        <div class="form-group">
            <label for="EmailAddress">Email Address</label>
            <input type="text" class="form-control" id="EmailAddress" placeholder="">
        </div>
      </fieldset>

      <button type="save" class="btn btn-default">Save</button>
      <button type="cancel" class="btn btn-default">Cancel</button>
    </form>
  </div>
  <div class="panel-footer">
    <button class="btn btn-primary .saveUSer">Save Changes</button>
    <button class="btn btn-link .cancelUser">Cancel</button>
  </div>
</div>